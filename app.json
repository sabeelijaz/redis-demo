{
  "name": "Node.js Redis demo",
  "description": "A demo setup for Redis",
  "repository": "https://github.com/velizarn/redis-demo.git",
  "logo": "https://cdn.jsdelivr.net/gh/heroku/node-js-getting-started@master/public/node.svg",
  "keywords": ["node", "express", "redis"],
  "image": "heroku/nodejs",
  "env": {
    "CACHE_TTL": {
      "description": "TTL for cached values",
      "value": "600",
      "required": true
    },
    "REDIS_URL": {
      "description": "The URL of the Redis server",
      "value": "",
      "required": true
    },
	"REDIS_PASSWORD": {
      "description": "If set, client will run Redis auth command on connect. ",
      "value": ""
    }
  }
}
